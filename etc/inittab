::sysinit:/etc/init.d/rcS

::ctrlaltdel:/sbin/reboot
::shutdown:/etc/init.d/rcK

tty1::respawn:/usr/bin/getty 38400 tty1
tty2::respawn:/usr/bin/getty 38400 tty2
tty3::respawn:/usr/bin/getty 38400 tty3
tty4::respawn:/usr/bin/getty 38400 tty4

ttyS0::respawn:/usr/bin/getty 115200 ttyS0

console::respawn:/sbin/getty -L console 0 vt100
